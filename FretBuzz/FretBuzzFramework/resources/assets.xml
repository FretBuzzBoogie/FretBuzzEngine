          <!-- KNIFE START-->
            <anim_state id="KnifeIdle" spritesheet_id="player_knife|knife_walk" is_loop="true" time_per_sprite="0.07" trigger="attack|KnifeAttack;" />
            <anim_state id="KnifeAttack" spritesheet_id="player_knife|knife_attack" is_loop="false" time_per_sprite="0.07" trigger="idle|KnifeIdle;" onCompleteTrigger="idle"/>
          <!-- KNIFE END-->

          <!-- DOUBLE BARREL START-->
